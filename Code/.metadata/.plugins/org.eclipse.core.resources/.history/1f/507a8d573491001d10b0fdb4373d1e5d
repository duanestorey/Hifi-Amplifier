#ifndef SRC_DAC_H_
#define SRC_DAC_H_

class DAC {
public:
	DAC();
	virtual ~DAC();

	virtual bool supportsVolume() { return false; }
	virtual bool supportsMute() { return false; }
	virtual int channels() { return 0; }

	virtual void setVolume( int channel, int volume ) {}

};

#endif /* SRC_DAC_H_ */
